<section style="margin-top: 50px;">
  <div class="container container-secondary">
    <div id="welcome" class="row">
      <div class="col-xs-12">
        <h1>Import Licenses</h1>
      </div>
    </div>
    <div class="row">
      <%= form_tag account_admin_import_licenses_path, multipart: true do %>
        <div class="form-group">
          <label>Upload xlsx file to assign licenses (columns: email, product_id, quantity)</label>
          <%= file_field_tag :file %>
        </div>
        <%= submit_tag "Upload", class: 'btn btn-primary' %>
      <% end %>
    </div>
  </div>
</section>
