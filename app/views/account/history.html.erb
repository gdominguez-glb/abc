<div class="container">
  <div class="history-list-wrapper">
    <div class="history-list-head clearfix">
      <span class="title pull-left">History</span>
      <div class="dropdown pull-right">
        Sort by:
        <button class="btn btn-default dropdown-toggle" type="button" id="dropdownMenu1" data-toggle="dropdown" aria-expanded="true">
          All
          <span class="caret"></span>
        </button>
        <ul class="dropdown-menu" role="menu" aria-labelledby="dropdownMenu1">
          <li role="presentation"><a role="menuitem" tabindex="-1" href="#">Action</a></li>
          <li role="presentation"><a role="menuitem" tabindex="-1" href="#">Another action</a></li>
          <li role="presentation"><a role="menuitem" tabindex="-1" href="#">Something else here</a></li>
          <li role="presentation"><a role="menuitem" tabindex="-1" href="#">Separated link</a></li>
        </ul>
      </div>
    </div>
    <table class="table">
      <thead>
        <tr>
          <th>Action</th>
          <th>Title</th>
          <th>Item</th>
          <th>Date</th>
          <th colspan="2"></th>
        </tr>
      </thead>
      <tbody>
        <% 1.upto(10).each do |i| %>
          <tr>
            <td><span class="glyphicon glyphicon-eye-open"></span></td>
            <td>Eureka Math</td>
            <td>Module 2, Grade 8, Lesson 3</td>
            <td>May 4, 2015</td>
            <td><a href="" class="btn btn-primary">View Item</a></td>
            <td><span class="glyphicon glyphicon-remove-sign"></span></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>
