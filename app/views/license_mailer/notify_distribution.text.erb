<% if @quantity > 1 %>
  <%= render partial: 'new_or_exist_multiple_license', locals: { admin_full_name: @school_admin.full_name, licenses_text: "#{@quantity} licenses for #{@product_name}" } %>
<% else %>
  <%= render partial: (@to_user.present? ? 'exist_user_single_license' : 'new_user_single_license'), locals: { product_name: @product_name, user_first_name: @to_user.try(:first_name) } %>
<% end %>
