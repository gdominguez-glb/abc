<% if @multiple %>
  <%= render partial: 'new_or_exist_multiple_license', locals: { admin_full_name: 'Great Minds Customer Service', licenses_text: @order.line_items.map{|li| "#{li.quantity} licenses for #{li.variant.product.name}"}.join(', ') } %>
<% else %>
  <%= render partial: (@order.user.present? ? 'exist_user_single_license' : 'new_user_single_license'), locals: { product_name: @product_names, user_first_name: @order.user.try(:first_name) } %>
<% end %>
