<% content_for :page_title do %>
  Coupon Code / <%= link_to @coupon_code.code, admin_coupon_code_path(@coupon_code)  %> / <%= @product.name %>
<% end %>

<table class="table">
  <thead>
    <tr>
      <th>Email</th>
    </tr>
  </thead>
  <tbody>
    <% @emails.each do |email| %>
      <tr>
        <td><%= email %></td>
      </tr>
    <% end %>
  </tbody>
</table>
