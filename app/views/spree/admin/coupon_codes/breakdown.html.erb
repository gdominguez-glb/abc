<% content_for :page_title do %>
  Product Key / <%= link_to @coupon_code.code, admin_coupon_code_path(@coupon_code)  %> / <%= @product.name %>
<% end %>

<div class="table-responsive">
  <table class="table">
    <thead>
      <tr>
        <th>Email</th>
      </tr>
    </thead>
    <tbody>
      <% @emails.each do |email| %>
        <tr>
          <td><%= email %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
