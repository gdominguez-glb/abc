<%= render 'cms_nav' %>
<div class="col-md-9">
  <dl class="dl-horizontal">
    <dt>Title</dt>
    <dd><%= @page.title %></dd>

    <dt>Seo content</dt>
    <dd><%= @page.seo_content %></dd>

    <dt>Slug</dt>
    <dd><%= @page.slug %></dd>

    <dt>Curriculum</dt>
    <dd><%= @page.curriculum.try(:name) %></dd>

    <dt>Group name</dt>
    <dd><%= @page.group_name %></dd>

    <dt>Sub group name</dt>
    <dd><%= @page.sub_group_name %></dd>

    <dt>Position</dt>
    <dd><%= @page.position %></dd>

    <dt>Layout</dt>
    <dd><%= @page.layout %></dd>

    <dt>Body</dt>
    <dd><%= @page.body %></dd>

    <dt>Visible</dt>
    <dd><%= @page.visible %></dd>

    <dt></dt>
    <dd>
      <%= link_to 'Edit', edit_cms_page_path(@page), class: 'btn btn-default' %>
      <%= link_to 'Back', cms_pages_path, class: 'btn btn-default' %>
    </dd>
  </dl>
</div>
