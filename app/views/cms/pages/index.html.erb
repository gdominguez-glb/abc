<div class="col-xs-7">
  <p id="notice"><%= notice %></p>
  <table>
    <thead>
      <tr>
        <th>Title</th>
        <th>Seo content</th>
        <th>Slug</th>
        <th>Group name</th>
        <th>Sub group name</th>
        <th>Position</th>
        <th>Layout</th>
        <th>Body</th>
        <th>Visible</th>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <% @pages.each do |page| %>
        <tr>
          <td><%= page.title %></td>
          <td><%= page.seo_content %></td>
          <td><%= page.slug %></td>
          <td><%= page.group_name %></td>
          <td><%= page.sub_group_name %></td>
          <td><%= page.position %></td>
          <td><%= page.layout %></td>
          <td><%= page.body %></td>
          <td><%= page.visible %></td>
          <td><%= link_to 'Show', page %></td>
          <td><%= link_to 'Edit', edit_page_path(page) %></td>
          <td><%= link_to 'Destroy', page, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
      <% end %>
    </tbody>
  </table>

  <br>

  <%= link_to 'New Page', new_cms_page_path %>
</div>
